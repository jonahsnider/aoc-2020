{
	"author": {
		"email": "jonah@jonah.pw",
		"name": "Jonah Snider",
		"url": "https://jonah.pw"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"ts"
			],
			"rewritePaths": {
				"src/": "tsc_output/"
			}
		}
	},
	"bugs": {
		"url": "https://github.com/pizzafox/aoc-2020/issues"
	},
	"dependencies": {
		"@pizzafox/benchmark": "1.1.0",
		"tslog": "3.0.1"
	},
	"devDependencies": {
		"@ava/typescript": "1.1.1",
		"@istanbuljs/nyc-config-typescript": "1.0.1",
		"@tsconfig/node14": "1.0.0",
		"@types/node": "14.14.9",
		"ava": "3.13.0",
		"eslint-plugin-prettier": "3.1.4",
		"nyc": "15.1.0",
		"prettier": "2.2.1",
		"prettier-config-xo": "1.0.3",
		"source-map-support": "0.5.19",
		"ts-node": "9.0.0",
		"typescript": "4.1.2",
		"xo": "0.35.0"
	},
	"license": "Apache-2.0",
	"main": "./tsc_output/solve.js",
	"name": "aoc-2020",
	"nyc": {
		"all": true,
		"extends": "@istanbuljs/nyc-config-typescript",
		"include": [
			"src/**/*.ts"
		],
		"reporter": [
			"lcov",
			"cobertura"
		]
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pizzafox/aoc-2020.git"
	},
	"scripts": {
		"benchmark": "node tsc_output/benchmark.js",
		"build": "tsc",
		"lint": "xo",
		"pretest": "rm -rf coverage .nyc_output",
		"solve": "node tsc_output/solve.js",
		"start": "node .",
		"style": "prettier --ignore-path .gitignore --check .",
		"test": "nyc ava"
	},
	"version": "1.0.0",
	"xo": {
		"prettier": true
	}
}
